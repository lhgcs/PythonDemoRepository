
QWidget {
    background:rgb(255,255,255);
}
QLabel {
    color:rgb(0,0,0);
    background:rgba(255,255,255,0);
}

QLabel#labelTitle {
    font:28px;
    color:rgb(255,255,255);
    background:rgba(0,0,0,0);
    border:none;
}

QLabel#Toast {
    font:26px;
    color:rgb(255,255,255);
    background:rgba(0,163,229,125);
    border-radius:12px;
    border:none;
    padding:20px;
}

QWidget#baseWidget, #baseStackedWidget {
    background:rgb(255,255,255);
}
QWidget#MainWindow {
    border-image:url(/home/ico/main.png);
}
QWidget#DialogInstance {
    border-image:url(/home/ico/dialog.png);
}
QWidget[objectName=DialogInstance] QLabel{
    font-size: 25px;
    color:rgb(0,0,0);
    background:rgba(255,255,255,0);
}
QWidget[objectName=DialogInstance] QPushButton{
    color:rgba(0,0,0,0);
    background:rgba(255,255,255,0);
}
QWidget[objectName=Indicator] {
    color:rgba(0,0,0,0);
    background:rgba(0,255,0,255);
}
QWidget#ParamterWidget {
    font-size:25px;
    background-color:rgb(56,73,89);
}
QWidget[objectName=ParamterWidget] QPushButton {
    color:rgb(255,255,255);
    background: rgb(239,61,87);
    padding:8px;
}
QWidget[objectName=ParamterWidget] QPushButton:pressed {
    color:rgb(255,255,255);
    background: rgb(239,201,70);
    padding:8px;
    padding-left:3px;
    padding-top:3px;
}


QWidget#LabelAndEdit {
    font-size:25px;
    background-color:rgba(0,0,0,0);
}
QWidget[objectName=LabelAndEdit] QLabel {
    font-size:25px;
    border: none;
    color:rgb(239,201,70);
    background: rgba(0,0,0,0);
}
QWidget[objectName=LabelAndEdit] QLineEdit {
