QWidget#Login {
    background-color:rgb(56,73,89);
}
QWidget[objectName=Login] QLabel {
    font-size: 25px;
    border: none;
    color:rgb(255,255,255);
    background: rgba(0,0,0,0);
}
QWidget[objectName=Login] QLineEdit {
    font-size: 25px;
    color:rgb(0,0,0);
    background: rgba(255,255,255,255);
    border:0px solid white;
}
QWidget[objectName=Login] QPushButton {
    font-size: 25px;
    color:rgb(255,255,255);
    background: rgba(0,0,0,0);
    border:1px solid white;
    padding:8px;
}
QWidget[objectName=Login] QPushButton:pressed {
    color:rgb(255,255,255);
    background: rgb(239,201,70);
    padding:8px;
    padding-right:4px;
    padding-bottom:4px;
}
