/**********按钮**********/
QPushButton {
    font-size: 24px;
    border: none;
    color: rgb(29,133,165);
    background: transparent;
}
QPushButton:hover {
    color: rgb(36,164,206);
}
QPushButton:pressed {
    padding-left:3px;
    padding-top:3px;
}

QPushButton#btnNormal {
    font-size: 24px;
    border:2px solid rgb(29,133,165);
    color: rgb(29,133,165);
    background: transparent;
}
QPushButton#btnNormal:pressed {
    padding-left:3px;
    padding-top:3px;
    color: rgb(255,255,255);
    background:rgb(29,133,165);
}

/*不限制作用域，只要控件名称为objectName时设置该样式
*/

QTableView {
    font-size:24px;
    color: white;                                       /*表格内文字颜色*/
    gridline-color: rgb(255,255,255);                   /*表格内框颜色*/
    background-color: rgba(0,0,0,0);                    /*表格内背景色*/
    alternate-background-color: rgb(68,182,159);
    selection-color: rgb(0,0,0);                        /*选中区域的文字颜色*/
    selection-background-color: rgb(239,201,70);        /*选中区域的背景色*/
    border: 2px groove gray;
    border-radius: 0px;
}

QHeaderView {
    color: black;
    font: bold 24px;/*16pt*/
    background-color: rgb(255,255,255);/*设置表头空白区域背景色*/
    border: 0px solid rgb(144, 144, 144);
    border-left-color:rgba(255, 255, 255, 0);
    border-top-color: rgba(255, 255, 255, 0);
    border-radius:0px;
    min-height:29px;
}
